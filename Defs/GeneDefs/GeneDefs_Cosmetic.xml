	<GeneDef ParentName="FertilityBase">
		<defName>RBM_EstrousCycle</defName>
		<label>estrous cycle</label>
		<description>Carriers of this gene go into heat during Aprimay. While in heat, they will crave lovin' more than usual and become more emotionally capricious.</description>
		<biostatCpx>1</biostatCpx>
		<minAgeActive>18</minAgeActive>
		<displayCategory>Reproduction</displayCategory>
		<displayOrderInCategory>115</displayOrderInCategory>
		<iconPath>UI/GeneIcons/RBM_EstrousCycle</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>rut</symbol></li>
				<li><symbol>heat</symbol></li>
				<li><symbol>spring</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>dosis</symbol></li>
				<li><symbol>mone</symbol></li>
				<li><symbol>mate</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<GeneDef>
		<defName>RBM_Lactation</defName>
		<label>lactation</label>
		<description>Carriers of this gene naturally produce milk.</description>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<minAgeActive>18</minAgeActive>
		<exclusionTags>
			<li>Lactation</li>
		</exclusionTags>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>600</displayOrderInCategory>
		<iconPath>UI/GeneIcons/RBM_Lactation</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<randomChosen>true</randomChosen>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>mam</symbol></li>
				<li><symbol>estro</symbol></li>
				<li><symbol>galacta</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>gogue</symbol></li>
				<li><symbol>mary</symbol></li>
				<li><symbol>der</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
  
	
	<GeneDef>
		<defName>RBM_IncreasedLactation</defName>
		<label>increased lactation</label>
		<description>Carriers of this gene produce a large amount of milk, but experience pain if unmilked for more than a day.</description>
		<biostatCpx>2</biostatCpx>
		<biostatMet>-2</biostatMet>
		<minAgeActive>18</minAgeActive>
		<exclusionTags>
			<li>Lactation</li>
		</exclusionTags>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>601</displayOrderInCategory>
		<iconPath>UI/GeneIcons/RBM_IncreasedLactation</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<randomChosen>true</randomChosen>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>giga</symbol></li>
				<li><symbol>estro</symbol></li>
				<li><symbol>ud</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>gogue</symbol></li>
				<li><symbol>mary</symbol></li>
				<li><symbol>der</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>


 

</Defs>