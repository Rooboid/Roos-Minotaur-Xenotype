<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- Cosmetic -->

	<!-- Bovine Ears -->

	<GeneDef ParentName="GeneEarsBase">
		<defName>RBM_BovineEars</defName>
		<label>bovine ears</label>
		<description>Carriers of this gene possess large, bovine ears.</description>
		<iconPath>UI/GeneIcons/RBM_BovineEars</iconPath>
		<graphicData>
			<graphicPaths>
				<li>Things/Pawn/Humanlike/Ears/RBM_Ears1/RBM_Ears1</li>
				<li>Things/Pawn/Humanlike/Ears/RBM_Ears2/RBM_Ears2</li>
				<li>Things/Pawn/Humanlike/Ears/RBM_Ears3/RBM_Ears3</li>
				<li>Things/Pawn/Humanlike/Ears/RBM_Ears4/RBM_Ears4</li>
				<li>Things/Pawn/Humanlike/Ears/RBM_Ears5/RBM_Ears5</li>
			</graphicPaths>
			<colorType>Skin</colorType>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>bo</symbol></li>
				<li><symbol>moo</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>vine</symbol></li>
				<li><symbol>moo</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- Bovine Horns -->

	<GeneDef ParentName="GeneHeadboneBase">
		<defName>RBM_BovineHorns</defName>
		<label>bovine horns</label>
		<description>Carriers of this gene sport a pair of formidable horns atop their head. It's not about how big they are, it's about how you use them.</description>
		<iconPath>UI/GeneIcons/RBM_BovineHorns</iconPath>
		<graphicData>
			<graphicPaths>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns1/RBM_Horns1</li>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns2/RBM_Horns2</li>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns3/RBM_Horns3</li>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns4/RBM_Horns4</li>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns5/RBM_Horns5</li>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns6/RBM_Horns6</li>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns7/RBM_Horns7</li>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns8/RBM_Horns8</li>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns9/RBM_Horns9</li>
				<li>Things/Pawn/Humanlike/Horns/RBM_Horns10/RBM_Horns10</li>
			</graphicPaths>
			<color>(255, 255, 255)</color>
			<layer>PostHeadgear</layer>
			<drawNorthAfterHair>true</drawNorthAfterHair>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>bull</symbol></li>
				<li><symbol>cow</symbol></li>
				<li><symbol>bovine</symbol></li>
				<li><symbol>tauru</symbol></li>
				<li><symbol>mino</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>stirk</symbol></li>
				<li><symbol>taur</symbol></li>
				<li><symbol>steer</symbol></li>
				<li><symbol>stock</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- Bovine Head -->
	
	<GeneDef ParentName="GeneJawBase">
		<defName>RBM_BovineHead</defName>
		<label>bovine head</label>
		<labelShortAdj>bovine</labelShortAdj>
		<description>Carriers of this gene possess a bovine head. They will oppose eating beef.</description>
		<iconPath>UI/GeneIcons/RBM_BovineHead</iconPath>
		<biostatMet>1</biostatMet>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>98</displayOrderInCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<forcedHeadTypes>
			<li>RBM_AverageHeadF1</li>
			<li>RBM_AverageHeadF2</li>
			<li>RBM_AverageHeadF3</li>
			<li>RBM_AverageHeadM1</li>
			<li>RBM_AverageHeadM2</li>
			<li>RBM_AverageHeadM3</li>
			<li>RBM_NarrowHeadF1</li>
			<li>RBM_NarrowHeadF2</li>
			<li>RBM_NarrowHeadF3</li>
			<li>RBM_NarrowHeadM1</li>
			<li>RBM_NarrowHeadM2</li>
			<li>RBM_NarrowHeadM3</li>
		</forcedHeadTypes>
		<!-- BeefEat Thoughts
		        <extraThoughtsFromIngestion>
          <li>
            <meatSource>Humanlike</meatSource>
            <thoughtsDirect>
              <li>AteHumanlikeMeatDirectCannibal</li>
            </thoughtsDirect>
            <thoughtsAsIngredient>
              <li>AteHumanlikeMeatAsIngredientCannibal</li>
            </thoughtsAsIngredient>
          </li>
        </extraThoughtsFromIngestion>  -->
		<statOffsets>
			<ComfyTemperatureMin>-3</ComfyTemperatureMin>
			<EatingSpeed>0.5</EatingSpeed>
		</statOffsets>
		<missingGeneRomanceChanceFactor>0.5</missingGeneRomanceChanceFactor>
		<!-- NoseRing -->
		<graphicData>
			<graphicPaths>
				<li>Things/Pawn/Humanlike/Markings/Head_NoseRing/Ring</li>
				<li>Things/Pawn/Humanlike/Markings/Head_Blank/Blank</li>
			</graphicPaths>
			<visibleNorth>false</visibleNorth>
			<layer>PostTattoo</layer>
		</graphicData>
		<!-- Symbols -->
		<symbolPack>
			<prefixSymbols>
				<li><symbol>bull</symbol></li>
				<li><symbol>cow</symbol></li>
				<li><symbol>bovine</symbol></li>
				<li><symbol>tauru</symbol></li>
				<li><symbol>mino</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>stirk</symbol></li>
				<li><symbol>taur</symbol></li>
				<li><symbol>steer</symbol></li>
				<li><symbol>stock</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- Earthy Skin-->
	
	<GeneDef ParentName="GeneSkinColorOverride">
		<defName>RBM_EarthySkin</defName>
		<label>earthy skin</label>
		<labelShortAdj>earthen</labelShortAdj>
		<description>Carriers of this gene possess skin of mild and earthy colours. Some carriers also possess bovine markings on their faces.</description>
		<randomBrightnessFactor>5</randomBrightnessFactor>
		<skinColorOverride>(200, 140, 110)</skinColorOverride>
		<displayOrderInCategory>99</displayOrderInCategory>
		<graphicData>
			<graphicPaths>
				<li>Things/Pawn/Humanlike/Markings/Head_Bull1/Bull1</li>
				<li>Things/Pawn/Humanlike/Markings/Head_Bull2/Bull2</li>
				<li>Things/Pawn/Humanlike/Markings/Head_Bull3/Bull3</li>
				<li>Things/Pawn/Humanlike/Markings/Head_Bull4/Bull4</li>
				<li>Things/Pawn/Humanlike/Markings/Head_Blank/Blank</li>
			</graphicPaths>
			<visibleNorth>false</visibleNorth>
			<layer>PostSkin</layer>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>terra</symbol></li>
				<li><symbol>earthen</symbol></li>
				<li><symbol>spot</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>cotta</symbol></li>
				<li><symbol>firma</symbol></li>
				<li><symbol>taur</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- Vocal -->
	
	<GeneDef ParentName="GeneVoiceBase">
		<defName>RBM_BovineVoice</defName>
		<label>bovine voice</label>
		<description>Holders of this gene will often moo when pleased or exhale a loud puff from their nostrils when agitated.</description>
		<iconPath>UI/GeneIcons/RBM_BovineVoice</iconPath>
		<displayOrderInCategory>135</displayOrderInCategory>
		<soundCall>RBM_BovineCall</soundCall>
		<soundDeath>RBM_BovineDeath</soundDeath>
		<soundWounded>RBM_BovineWounded</soundWounded>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>bull</symbol></li>
				<li><symbol>cow</symbol></li>
				<li><symbol>bovine</symbol></li>
				<li><symbol>tauru</symbol></li>
				<li><symbol>mino</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>stirk</symbol></li>
				<li><symbol>taur</symbol></li>
				<li><symbol>steer</symbol></li>
				<li><symbol>stock</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- Functional -->
		
	<GeneDef>
		<defName>RBM_UnguligradeLegs</defName>
		<label>unguligrade legs</label>
		<description>Carriers of this gene have sturdy, unguligrade legs with cloven hooves, perfect for hotfooting it out of trouble, but not-so-perfect for keeping the floor clean. The fur on their legs extends up to their stomachs, covering up their modesty.</description>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>330</displayOrderInCategory>
		<iconPath>UI/GeneIcons/RBM_BovineLegs</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<statOffsets>
			<StaggerDurationFactor>-0.5</StaggerDurationFactor>
			<MoveSpeed>0.2</MoveSpeed>
			<FilthRate>1.3</FilthRate>
			<MeleeDodgeChance>5</MeleeDodgeChance>
		</statOffsets>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>faun</symbol></li>
				<li><symbol>jog</symbol></li>
				<li><symbol>bov</symbol></li>
				<li><symbol>bolt</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>yr</symbol></li>
				<li><symbol>ine</symbol></li>
				<li><symbol>trotter</symbol></li>
				<li><symbol>hoof</symbol></li>
				<li><symbol>ped</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
  
	<GeneDef>
		<defName>RBM_Herculean</defName>
		<label>Herculean</label>
		<description>Carriers of this gene stand at over seven feet tall. Their herculean bodies prove them to be excellent pack mules, formidable foes in battle, and rather painful lovers. There are some weapons that only Herculean pawns can lift.</description>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-3</biostatMet>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>580</displayOrderInCategory>
		<iconPath>UI/GeneIcons/RBM_Herculean</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<randomChosen>true</randomChosen>
		<statOffsets>
			<MeleeDodgeChance>-10</MeleeDodgeChance>
			<CarryingCapacity>4</CarryingCapacity>
			<MoveSpeed>-0.2</MoveSpeed>
		</statOffsets>
		<statFactors>
			<MeatAmount>1.5</MeatAmount>
			<LeatherAmount>1.5</LeatherAmount>
			<ArrestSuccessChance>1.5</ArrestSuccessChance>
			<MeleeDPS>1.3</MeleeDPS>
		</statFactors>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>rip</symbol></li>
				<li><symbol>wall</symbol></li>
				<li><symbol>sky-</symbol></li>
				<li><symbol>hench</symbol></li>
				<li><symbol>jack</symbol></li>
				<li><symbol>aggro</symbol></li>
				<li><symbol>vigor</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>roid</symbol></li>
				<li><symbol>lean</symbol></li>
				<li><symbol>bulk</symbol></li>
				<li><symbol>rile</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
		<GeneDef>
		<defName>RBM_BovineTemper</defName>
		<label>Bovine Temper</label>
		<description>Carriers of this gene have a short temper and become frighteneningly aggressive when enraged. They possess the ability to 'See Red', a horrifying state of outrage wherin the user unleashes devastating attacks upon anyone who stands in their way.</description>
		<biostatCpx>0</biostatCpx>
		<biostatMet>1</biostatMet>
		<displayCategory>Ability</displayCategory>
		<displayOrderInCategory>200</displayOrderInCategory>
		<iconPath>UI/GeneIcons/RBM_Bull</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<abilities>
			<li>RBM_SeeRed</li>
		</abilities>
		<descriptionHyperlinks>
			<AbilityDef>RBM_SeeRed</AbilityDef>
		</descriptionHyperlinks>
		<socialFightChanceFactor>1.2</socialFightChanceFactor>
		<aggroMentalBreakSelectionChanceFactor>3</aggroMentalBreakSelectionChanceFactor>
		<prisonBreakMTBFactor>5</prisonBreakMTBFactor>
		<!-- Symbol -->
		<symbolPack>
		      <wholeNameSymbols>
        <li><symbol>minotaur</symbol></li>
      </wholeNameSymbols>
			<prefixSymbols>
				<li><symbol>bull</symbol></li>
				<li><symbol>steer</symbol></li>
				<li><symbol>an</symbol></li>
				<li><symbol>mino</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>vine</symbol></li>
				<li><symbol>gus</symbol></li>
				<li><symbol>ford</symbol></li>
				<li><symbol>taur</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

	<!-- TODO -->
	
	<GeneDef>
		<defName>RBM_RuminantStomach</defName>
		<label>ruminant stomach</label>
		<description>Carriers of this gene grow an intricate digestive system- slightly more hardy than a multi-fold stomach. They can digest plant matter and haygrass with ease.</description>
		<biostatCpx>2</biostatCpx>
		<biostatMet>-2</biostatMet>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>11</displayOrderInCategory>
		<iconPath>UI/GeneIcons/RBM_RuminantStomach</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<dontMindRawFood>true</dontMindRawFood>
		<statFactors>
			<RawNutritionFactor>2</RawNutritionFactor>
			<MaxNutrition>1.5</MaxNutrition>
		</statFactors>
		<statOffsets>
			<ForagedNutritionPerDay>0.5</ForagedNutritionPerDay>
		</statOffsets>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>graze</symbol></li>
				<li><symbol>hay</symbol></li>
				<li><symbol>herbi</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>grazer</symbol></li>
				<li><symbol>vore</symbol></li>
				<li><symbol>dae</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
  
