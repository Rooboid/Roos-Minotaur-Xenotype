<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>RBM_MilkingMachine</defName>
    <label>milking machine</label>
    <description>A machine where pawns with the lactation gene can express and store milk.</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <fillPercent>0.3</fillPercent>
    <tickerType>Normal</tickerType>
    <drawGUIOverlay>true</drawGUIOverlay>
	 <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,1)</interactionCellOffset>
    <graphicData>
      <texPath>Things/Building/RBM_MilkingMachine</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shadowData>
        <volume>(1,0.5,1)</volume>
      </shadowData>
      <damageData>
        <rect>(0,0,1,1)</rect>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
	<costList>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <WorkToBuild>5000</WorkToBuild>
      <Flammability>0</Flammability>
      <Mass>10</Mass>
    </statBases>
    <comps>
      <li Class="CompProperties_ThingContainer">
        <compClass>CompEggContainer</compClass>
        <stackLimit>25</stackLimit>
        <minCountToEmpty>20</minCountToEmpty>
        <drawStackLabel>true</drawStackLabel>
      </li>
	  <li Class="CompProperties_Flickable"/>
	  <li Class="CompProperties_Breakdownable"/>
    </comps>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>200</costStuffCount>
    <pathCost>10</pathCost>
    <designationCategory>Misc</designationCategory>
    <uiOrder>2060</uiOrder>
    <rotatable>false</rotatable>
    <building>
      <paintable>true</paintable>
    </building>
	 <researchPrerequisites>
      <li>Electricity</li>
    </researchPrerequisites>
  </ThingDef>
  
