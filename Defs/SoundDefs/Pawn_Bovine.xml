					<li Class="AudioGrain_Folder">
						<clipFolderPath>RBM_BovineDeath</clipFolderPath>
					</li>
				</grains>
				<volumeRange>10</volumeRange>
				<pitchRange>0.95~1.05</pitchRange>
				<sustainLoop>False</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>

	<SoundDef>
		<defName>RBM_BovineWounded</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>RBM_BovineWounded</clipFolderPath>
					</li>
				</grains>
				<volumeRange>10</volumeRange>
				<pitchRange>0.95~1.05</pitchRange>
				<sustainLoop>False</sustainLoop>
				<repeatMode>NeverTwice</repeatMode>
			</li>
		</subSounds>
	</SoundDef>
	
		<!-- See Red - Ability Sounds -->

	<SoundDef>
    <defName>SeeRed_Cast</defName>
    <context>MapOnly</context>
    <maxSimultaneous>2</maxSimultaneous>
    <priorityMode>PrioritizeNearest</priorityMode>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>RBM_SeeRed/Cast</clipFolderPath>
          </li>
        </grains>
        <volumeRange>70</volumeRange>
        <muteWhenPaused>true</muteWhenPaused>
        <tempoAffectedByGameSpeed>true</tempoAffectedByGameSpeed>
      </li>
    </subSounds>
  </SoundDef>
  
