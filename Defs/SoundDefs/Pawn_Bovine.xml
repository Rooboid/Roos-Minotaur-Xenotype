<Defs>

	<!-- Bovine Voice Sounds -->

	<SoundDef>
		<defName>RBM_BovineCall</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>RBM_BovineCall</clipFolderPath>
					</li>
				</grains>
				<volumeRange>10</volumeRange>
				<pitchRange>0.95~1.05</pitchRange>
				<sustainLoop>False</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>

	<SoundDef>
		<defName>RBM_BovineDeath</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
