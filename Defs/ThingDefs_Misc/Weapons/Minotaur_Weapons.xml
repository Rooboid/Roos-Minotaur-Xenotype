<Defs>


	<!--====================== Bullslinger ======================-->
	
	<ThingDef ParentName="BaseWeaponNeolithic">
		<defName>RBM_Bullslinger</defName>
		<label>bullslinger</label>
		<description>An ancient, obsidian bow of unknown origin, passed down by countless generations of minotaur. It fires blisteringly cold arrows, accurate enough to trim the nosehair of a pigskin from five acres away, providing that the user is absolutely focused. It is so heavy that only pawns with the Herculean gene may equip it.</description>
		<descriptionHyperlinks>
			<GeneDef>RBM_Herculean</GeneDef>
		</descriptionHyperlinks>
		<!-- Basic Data -->
		<possessionCount>1</possessionCount>
		<rotateInShelves>false</rotateInShelves>
		<uiIconScale>0.8</uiIconScale>
		<graphicData>
			<texPath>Things/Item/Equipment/WeaponRanged/RBM_Bullslinger/RBM_Bullslinger</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1.5</drawSize>
		</graphicData>
		<!-- Trading -->
		<techLevel>Archotech</techLevel>
		<tradeTags>
			<li>ExoticMisc</li>
		</tradeTags>
		<tradeNeverStack>true</tradeNeverStack>
		<tradeability>Sellable</tradeability>
		<!-- Obtaining -->
		<recipeMaker IsNull="True"/>
		<soundInteract>RBM_Bullslinger_Equip</soundInteract>
		<relicChance>3</relicChance>
		<smeltable>false</smeltable>
		<thingSetMakerTags>
			<li>RewardStandardMidFreq</li>
		</thingSetMakerTags>
		<!-- Battle Data -->
		<statBases>
			<Mass>5.2</Mass>
			<MarketValue>750</MarketValue>
			<AccuracyTouch>0.45</AccuracyTouch>
			<AccuracyShort>0.69</AccuracyShort>
			<AccuracyMedium>0.87</AccuracyMedium>
			<AccuracyLong>0.96</AccuracyLong>
			<RangedWeapon_Cooldown>1.65</RangedWeapon_Cooldown>
			<MaxHitPoints>250</MaxHitPoints>
			<RoyalFavorValue>2</RoyalFavorValue>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.05</MoveSpeed>
		</equippedStatOffsets>
		<weaponTags>
			<li>HerculeanWeapon</li>
		</weaponTags>
		<weaponClasses>
			<li>Ranged</li>
			<li>RangedLight</li>
		</weaponClasses>
		<verbs>
			<li>
				<verbClass>Verb_Shoot</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>Proj_ObsidianArrow</defaultProjectile>
				<warmupTime>1.8</warmupTime>
				<range>35.5</range>
				<soundCast>Bow_Recurve</soundCast>
				<soundCastTail>RBM_Bullslinger_Fire</soundCastTail>
			</li>
		</verbs>
		<tools>
			<li>
				<label>limb</label>
				<capacities>
					<li>Blunt</li>
					<li>Poke</li>
				</capacities>
				<power>15</power>
				<cooldownTime>2</cooldownTime>
			</li>
		</tools>
		<!-- Comps -->
		<comps>
			<li Class="CompProperties_TargetEffect_FleckOnTarget">
				<fleckDef>HerculeanFleckA</fleckDef>
			</li>
			<li Class="CompProperties_Art">
				<nameMaker>NamerMinotaurArt</nameMaker>
				<descriptionMaker>ArtDescription_WeaponGun</descriptionMaker>
				<minQualityForArtistic>Excellent</minQualityForArtistic>
			</li>
		</comps>
	</ThingDef>
	
	
	<!-- Obsidian Arrow (Projectile fired by the Bullslinger) -->
	
